{
  "checks": {
    "check-process-neutron-metering-agent": {
      "command"     : "/etc/sensu/plugins/processes/check-procs.rb -p neutron-metering-agent -C 1",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    },
    "check-neutron-metering-agent": {
      "command"     : "/etc/sensu/plugins/openstack/neutron/neutron-agent-status.py",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    }
  }
}
