{
  "checks": {
    "check-http-cloudera-cm4-server": {
      "command"     : "/etc/sensu/plugins/http/check-http.rb -h 'localhost' -P '7180' -p '/'",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    },
    "check-process-cloudera-cm4-server": {
      "command"     : "/etc/sensu/plugins/processes/check-procs.rb -p 'com.cloudera.server.cmf.Main' -C 1",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    }
  }
}
