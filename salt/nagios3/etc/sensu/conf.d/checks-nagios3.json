{
  "checks": {
    "check-http-nagios3": {
      "command"     : "/etc/sensu/plugins/http/check-http.rb -h 'localhost' -P '80' -p '/'",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    },
    "check-process-nagios3": {
      "command"     : "/etc/sensu/plugins/processes/check-procs.rb -p nagios3 -C 1",
      "handlers"    : ["default"],
      "interval"    :   60,
      "standalone"  :   true
    }
  }
}
